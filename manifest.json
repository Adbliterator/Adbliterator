{
    "name": "Adbliterator",
    "version": "1.0.7",
    "description": "Removes All Ads From YouTube and YouTube Music",
    "manifest_version": 3,
    "author": "Segations, RareHyperionYT, Jass",
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "adbliterator.js"
            ],
            "all_frames": true
        }
    ],

    "action": {},

    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab", 
        "scripting"
    ]
   
    
}
